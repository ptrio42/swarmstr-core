{
  "name": "swarmstr-core",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ptrio42/swarmstr-core.git"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "scripts": {
    "rollup": "rollup -c",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build": "npm run build:esm && npm run build:cjs && cpx 'src/**/*.css' dist/esm && cpx 'src/**/*.css' dist/cjs",
    "prepare": "npm run build",
    "prepare:watch": "concurrently \"tsc -p tsconfig.esm.json --watch\" \"tsc -p tsconfig.cjs.json --watch\" \"cpx 'src/**/*.css' dist/ --watch\""
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "devDependencies": {
    "@types/lodash": "^4.17.10",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.11",
    "@types/throttle-debounce": "^5.0.2",
    "concurrently": "^9.0.1",
    "cpx": "^1.2.1",
    "tslib": "^2.7.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@giphy/react-components": "^9.6.0",
    "@mui/icons-material": "^6.1.3",
    "@mui/material": "^6.1.4",
    "@nostr-dev-kit/ndk": "^2.10.1",
    "@nostr-dev-kit/ndk-cache-dexie": "^2.5.2",
    "@uiw/react-md-editor": "^4.0.4",
    "axios": "^1.7.7",
    "domhandler": "^5.0.3",
    "emoji-picker-react": "^4.12.0",
    "formik": "^2.4.6",
    "html-react-parser": "^5.1.18",
    "javascript-time-ago": "^2.5.11",
    "lodash": "^4.17.21",
    "nostr-tools": "^2.8.0",
    "react": "^18.3.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-player": "^2.16.0",
    "react-qr-code": "^2.0.15",
    "react-time-ago": "^7.3.3",
    "robohash-avatars": "^1.0.1",
    "tiny-lru": "^11.2.11",
    "webln": "^0.3.2"
  }
}
